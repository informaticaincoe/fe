<odoo noupdate="1">

    <!-- ================= -->
    <!-- DTE Ventas -->
    <!-- ================= -->
    <record id="seq_dte_import_sale" model="ir.sequence">
        <field name="name">Importación DTE Ventas</field>
        <field name="code">dte.import.sale</field>
        <field name="prefix">IMP/V/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- ================= -->
    <!-- DTE Compras -->
    <!-- ================= -->
    <record id="seq_dte_import_purchase" model="ir.sequence">
        <field name="name">Importación DTE Compras</field>
        <field name="code">dte.import.purchase</field>
        <field name="prefix">IMP/C/%(year)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
