<odoo>
    <data>
        <record id="view_anexo_sujeto_excluido_list" model="ir.ui.view">
            <field name="name">account.move.anexo.sujeto.excluido.list</field>
            <field name="model">account.move</field>
            <field name="arch" type="xml">
                <list string="Anexos Sujeto excluido">
                    <header>
                        <button name="action_download_csv_anexo"
                                type="object"
                                string="Descargar CSV"
                                class="oe_highlight"
                                context="{'active_view_id': context.get('active_view_id') or context.get('view_id')}"
                                display="always"
                        />
                    </header>

                    <field name="codigo_tipo_documento_cliente_display" string="Tipo documento"/>
                    <field name="documento_sujeto_excluido" string="Número de documento"/>
                    <field name="razon_social" string="Nombre"/>
                    <field name="invoice_date" string="Fecha de Emisión"/>
                    <field name="hacienda_selloRecibido" string="Número de serie"/>
                    <field name="numero_documento" string="Número de documento"/>
                    <field name="total_operacion" string="Monto de la operacion"/>
                    <field name="retencion_iva_amount" string="Monto de la retencion IVA 13%"/>
                    <field name="tipo_operacion_display"/>
                    <field name="clasificacion_facturacion_display" string="Calificación"/>
                    <field name="sector_display"/>
                    <field name="tipo_costo_gasto_display"/>
                    <field name="numero_anexo"/>
                </list>
            </field>
        </record>
    </data>
</odoo>