<odoo>
    <data>
        <record id="view_anexo_compras_list" model="ir.ui.view">
            <field name="name">account.move.anexo.compras.list</field>
            <field name="model">account.move</field>
            <field name="arch" type="xml">
                <list string="Detalle de Anexos Compras">
                    <header>
                        <button name="action_download_csv_anexo"
                                type="object"
                                string="Descargar CSV"
                                class="oe_highlight"
                                display="always"
                        />
                    </header>
                    <field name="invoice_date" string="Fecha de Emisión"/>
                    <field name="clase_documento_display"/>
                    <field name="codigo_tipo_documento_display"/>
                    <field name="numero_documento" string="Número documento"/>
                    <field name="nit_o_nrc_anexo_contribuyentes"/>
                    <field name="razon_social"/>
                    <field name="compras_internas_total_excento"/>
                    <field name="internaciones_exentas_no_sujetas"/>
                    <field name="importaciones_exentas_no_sujetas"/>
                    <field name="compras_internas_gravadas"/>
                    <field name="internaciones_gravadas_bienes"/>
                    <field name="importaciones_gravadas_bienes"/>
                    <field name="importaciones_gravadas_servicio"/>
                    <field name="credito_fiscal"/>
                    <field name="total_compra"/>
                    <field name="dui_cliente" string="DUI del proveedor"/>
                    <field name="tipo_operacion_display"/>
                    <field name="clasificacion_facturacion_display"/>
                    <field name="sector_display"/>
                    <field name="tipo_costo_gasto_display"/>
                    <field name="numero_anexo"/>
                </list>
            </field>
        </record>
    </data>
</odoo>

