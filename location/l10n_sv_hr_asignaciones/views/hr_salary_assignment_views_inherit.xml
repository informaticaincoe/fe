<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <record id="hr_salary_assignment_tree_inherit_button" model="ir.ui.view">
      <field name="name">hr.salary.assignment.tree.inherit.download_button</field>
      <field name="model">hr.salary.assignment</field>
      <field name="inherit_id" ref="l10n_sv_hr_asignaciones.view_hr_salary_assignment_tree"/>
      <field name="arch" type="xml">
        <xpath expr="/list" position="inside">
          <header>
            <button name="action_descargar_plantilla"
                    type="object"
                    string="Descargar plantilla"
                    class="btn-secondary"
                    icon="fa-download"/>
          </header>
        </xpath>
      </field>
    </record>
  </data>
</odoo>
