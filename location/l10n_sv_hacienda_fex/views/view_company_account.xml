<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_company_exportacion_account" model="ir.ui.view">
        <field name="name">view.company.exportacion.account</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="priority">65</field>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='iva_percibido_account_id']" position="after">
                <field name="account_exportacion_id" string="Cuenta Seguro y Flete"/>
            </xpath>
        </field>
    </record>
</odoo>
